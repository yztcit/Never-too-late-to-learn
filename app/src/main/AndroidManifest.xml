<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nttn.coolandroid">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.nttn.coolandroid.activity.CustomApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_learn"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.nttn.coolandroid.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnui.uibase.UIBaseActivity">
            <intent-filter>
                <action android:name="${applicationId}.item0"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learncomponent.ComponentActivity">
            <intent-filter>
                <action android:name="${applicationId}.item1"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnlayout.LayoutActivity">
            <intent-filter>
                <action android:name="${applicationId}.item2"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learninteraction.InteractionActivity">
            <intent-filter>
                <action android:name="${applicationId}.item3"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learndata.activity.DataStorageActivity">
            <intent-filter>
                <action android:name="${applicationId}.item4"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnhttp.HttpActivity">
            <intent-filter>
                <action android:name="${applicationId}.item5"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnanimation.AnimationActivity">
            <intent-filter>
                <action android:name="${applicationId}.item6"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learndrawview.DrawViewActivity">
            <intent-filter>
                <action android:name="${applicationId}.item7"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnmedia.MediaActivity">
            <intent-filter>
                <action android:name="${applicationId}.item8"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnui.uiadvance.UIAdvancedActivity">
            <intent-filter>
                <action android:name="${applicationId}.item9"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnstyle.StyleActivity">
            <intent-filter>
                <action android:name="${applicationId}.item10"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnthreads.ThreadsActivity">
            <intent-filter>
                <action android:name="${applicationId}.item11"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
        </activity>
        <activity android:name="com.nttn.coolandroid.learnothers.OthersActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="${applicationId}.item12"/>

                <category android:name="android.intent.category.DEFAULT"/> <!-- 设置 default category... -->
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="nttn" android:host="nttn.com"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryImagesActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryAudiosActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryVideosActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryDocumentsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryArchivesActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryAppsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryDownloadsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learndata.activity.CategoryMoreActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.nttn.coolandroid.learncomponent.SecondActivity"/>
        <activity android:name="com.nttn.coolandroid.learnui.uiadvance.DragDemoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.nttn.coolandroid.learnui.uiadvance.HtmlTagActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.nttn.coolandroid.learncomponent.rxpractise.RxActivity"
            android:process=":com.chen.coolandroid.rxpractise"
            android:screenOrientation="portrait"/>
        <activity android:name="com.nttn.coolandroid.learnthreads.AIDLTestActivity" />

        <service android:name="com.nttn.coolandroid.learnthreads.RemoteService"
            android:process=":remote"
            android:enabled="true"
            android:exported="true"/>
    </application>

</manifest>